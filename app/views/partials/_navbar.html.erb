        <% else %>
            <li><%= link_to 'Log In', new_user_session_path, class: 'navLink' %></li>
            <li><%= link_to 'Sign Up', new_user_registration_path, class: 'navLink' %></li>
        <% end %>
      </ul>
    </div>
  </nav>
</header>