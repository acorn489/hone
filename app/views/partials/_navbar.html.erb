        <%= link_to "Hone", {:controller => '/welcome', :action => 'show'}, class: "navbar-brand" %>
      </div>
      <ul id="navList">
        <li><a class="navLink" href="#">Contact</a></li>
        <% if user_signed_in? %>
            <li><%= link_to 'Profile', edit_user_registration_path, class: 'navLink' %></li>
